{
  "Meeting notes": {
    "prefix": "meetingnotes",
    "body": [
      "# Meeting notes: ${1:Title} - ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "",
      "## Notes",
      "",
      "$2",
      "",
      "",
      "## Action items",
      "$3"
    ]
  },
  "conference talk notes": {
    "prefix": "talknotes",
    "body": [
      "# Talk: ${1:Title} - ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "Speaker: ${2:Name}",
      "",
      "## Notes",
      "",
      "$3"
    ]
  },
  "shell": {
    "prefix": "shell",
    "body": [
      "",
      "```shell",
      "$1",
      "```",
      ""
    ]
  },
  "python": {
    "prefix": "python",
    "body": [
      "",
      "```python",
      "$1",
      "```",
      ""
    ]
  }
}
