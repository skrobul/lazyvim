{
  "LOG statement (openstack)": {
    "prefix": "logo",
    "description": "Openstack style logging statement",
    "body": [
      "LOG.${1|debug,error,info,warning,critical,exception|}(\"${2:sometext} %(${3:var})s\", {\"$3\": $3})"
    ]
  },
  "LOG.debug (openstack)": {
    "prefix": "logd",
    "description": "Openstack style logging debug statement",
    "body": [
      "LOG.debug(\"${1:sometext} %(${2:var})s\", {\"$2\": $2})"
    ]
  },
  "LOG.info (openstack)": {
    "prefix": "logi",
    "description": "Openstack style logging info statement",
    "body": [
      "LOG.info(\"${1:sometext} %(${2:var})s\", {\"$2\": $2})"
    ]
  },
  "LOG.error (openstack)": {
    "prefix": "logi",
    "description": "Openstack style logging error statement",
    "body": [
      "LOG.error(\"${1:sometext} %(${2:var})s\", {\"$2\": $2})"
    ]
  }
}
